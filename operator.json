{
  "name": "Non Linear Regression",
  "description": "Fits different non-linear regression models to your data (e.g. for Dose-Response analysis).",
  "tags": [
    "statistical testing",
    "linear model"
  ],
  "authors": [
    "tercen"
  ],
  "urls": [
    "https://github.com/tercen/nonlinear_regression_operator"
  ],
  "properties": [
    {
      "kind": "EnumeratedProperty",
      "name": "function.type",
      "defaultValue": "Three-parameter log-logistic",
      "description": "Function to fit.",
      "values": [
        "Three-parameter log-logistic",
        "Four-parameter log-logistic",
        "Michaelis-Menten"
      ]
    },
    {
      "kind": "EnumeratedProperty",
      "name": "dose.transformation",
      "defaultValue": "None",
      "description": "Which transformation has already been applied to the dose values?",
      "values": [
        "None",
        "Log",
        "Log10"
      ]
    },
    {
      "kind": "DoubleProperty",
      "name": "n.predictions",
      "defaultValue": 100,
      "description": "Number of predicted values to generate."
    },
    {
      "kind": "StringProperty",
      "name": "response.output",
      "defaultValue": "50, 90",
      "description": "Comma-separated list of percentages of maximal response to generate predicted values for (for example, Y50 will give you the EC50 value as X50, Y50 being half the maximal response value)."
    },
    {
      "kind": "BooleanProperty",
      "name": "relative.response",
      "defaultValue": false,
      "description": "Whether to compute absolute (default) or relative effective doses."
    },
    {
      "kind": "DoubleProperty",
      "name": "maximum.x",
      "defaultValue": 1000000,
      "description": "Maximum value for the estimation of effective doses."
    }
  ],
  "operatorSpec": {
    "kind": "OperatorSpec",
    "ontologyUri": "https://tercen.com/_ontology/tercen",
    "ontologyVersion": "0.0.1",
    "inputSpecs": [
      {
        "kind": "CrosstabSpec",
        "metaFactors": [
          {
            "kind": "MetaFactor",
            "name": "Rows",
            "type": "",
            "description": "Factor(s) to stratify per row.",
            "ontologyMapping": "observation",
            "crosstabMapping": "row",
            "cardinality": "0..n",
            "factors": []
          },
          {
            "kind": "MetaFactor",
            "name": "Columns",
            "type": "",
            "description": "Factor(s) to stratify per column.",
            "ontologyMapping": "variable",
            "crosstabMapping": "column",
            "cardinality": "0..n",
            "factors": []
          }
        ],
        "axis": [
          {
            "kind": "AxisSpec",
            "metaFactors": [
              {
                "kind": "MetaFactor",
                "name": "Measurement Value",
                "type": "numeric",
                "description": "Measurement value (e.g. response)",
                "ontologyMapping": "measurement",
                "crosstabMapping": "y",
                "cardinality": "1",
                "factors": []
              },
              {
                "kind": "MetaFactor",
                "name": "Explanatory Value",
                "type": "numeric",
                "description": "Explanatory value (e.g. dose)",
                "ontologyMapping": "measurement",
                "crosstabMapping": "x",
                "cardinality": "1",
                "factors": []
              }
            ]
          }
        ]
      }
    ],
    "outputSpecsV2": [
      {
        "kind": "ConditionalJoinSpec",
        "description": "Output columns vary based on function.type property",
        "alternatives": [
          {
            "kind": "OutputAlternative",
            "condition": "function.type = 'Three-parameter log-logistic' (LL.3)",
            "joinSpec": {
              "kind": "OperatorJoinSpec",
              "joinOperators": [
                {
                  "kind": "JoinOperator",
                  "joinType": "left",
                  "leftPair": {
                    "kind": "ColumnPair",
                    "lColumns": [
                      "Rows",
                      "Columns"
                    ],
                    "rColumns": [
                      "Rows",
                      "Columns"
                    ]
                  },
                  "rightRelation": {
                    "kind": "TableRelation",
                    "description": "Fitted parameters and summary statistics (LL.3)",
                    "attributes": [
                      {
                        "kind": "Attribute",
                        "name": "b",
                        "type": "double"
                      },
                      {
                        "kind": "Attribute",
                        "name": "d",
                        "type": "double"
                      },
                      {
                        "kind": "Attribute",
                        "name": "e",
                        "type": "double"
                      },
                      {
                        "kind": "Attribute",
                        "name": "pseudo_R2",
                        "type": "double"
                      },
                      {
                        "kind": "Attribute",
                        "name": "X50",
                        "type": "double"
                      },
                      {
                        "kind": "Attribute",
                        "name": "Y50",
                        "type": "double"
                      },
                      {
                        "kind": "Attribute",
                        "name": "X90",
                        "type": "double"
                      },
                      {
                        "kind": "Attribute",
                        "name": "Y90",
                        "type": "double"
                      }
                    ]
                  }
                },
                {
                  "kind": "JoinOperator",
                  "joinType": "left",
                  "leftPair": {
                    "kind": "ColumnPair",
                    "lColumns": [
                      "Rows",
                      "Columns"
                    ],
                    "rColumns": [
                      "Rows",
                      "Columns"
                    ]
                  },
                  "rightRelation": {
                    "kind": "TableRelation",
                    "description": "Prediction curve points",
                    "attributes": [
                      {
                        "kind": "Attribute",
                        "name": "x_pred",
                        "type": "double"
                      },
                      {
                        "kind": "Attribute",
                        "name": "y_pred",
                        "type": "double"
                      }
                    ]
                  }
                }
              ]
            }
          },
          {
            "kind": "OutputAlternative",
            "condition": "function.type = 'Four-parameter log-logistic' (LL.4)",
            "joinSpec": {
              "kind": "OperatorJoinSpec",
              "joinOperators": [
                {
                  "kind": "JoinOperator",
                  "joinType": "left",
                  "leftPair": {
                    "kind": "ColumnPair",
                    "lColumns": [
                      "Rows",
                      "Columns"
                    ],
                    "rColumns": [
                      "Rows",
                      "Columns"
                    ]
                  },
                  "rightRelation": {
                    "kind": "TableRelation",
                    "description": "Fitted parameters and summary statistics (LL.4)",
                    "attributes": [
                      {
                        "kind": "Attribute",
                        "name": "b",
                        "type": "double"
